{
  "name": "@naiheyoung/prettier",
  "version": "0.0.5",
  "packageManager": "pnpm@10.28.0",
  "description": "personal prettier configuration.",
  "homepage": "https://github.com/naiheyoung/prettier-config#readme",
  "author": "Naihe",
  "repository": {
    "type": "git",
    "url": "https://github.com/naiheyoung/prettier-config.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "vitest",
    "prebuild": "pnpm format",
    "build": "tsdown",
    "format": "prettier --write src/",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "prettier",
    "config"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.3.3",
    "prettier": "^3.8.1",
    "tsdown": "0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  }
}
